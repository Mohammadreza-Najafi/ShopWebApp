<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" type="image/x-icon" href="~/Theme/assets/images/favicon.png" />
    <link href="~/Theme/assets/fonts.googleapis.com/css95e4.css?family=Josefin+Sans:300,300i,400,400i,600,600i,700,700i&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/animate.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/chosen.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/jquery.scrollbar.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/lightbox.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/magnific-popup.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/slick.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/fonts/flaticon.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/megamenu.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/dreaming-attribute.css" />
    <link rel="stylesheet" type="text/css" href="~/Theme/assets/css/style.css" />

    @if(ViewData["Title"] != null)
    {
        <title>@ViewData["Title"] - Shop</title>
    }
    else
    {
        <title>Shop</title>
    }
    
    <meta name="keywords" content="@ViewData["keywords"]" />
    <meta name="description" content="@ViewData["metaDescription"]" />

</head>
<body>

    @await Component.InvokeAsync("Menu")

    @RenderBody()

    @await Component.InvokeAsync("Footer")
     
    
    <a href="#" class="backtotop active">
        <i class="fa fa-angle-double-up"></i>
    </a>
    
    <script src="~/Theme/assets/js/jquery-1.12.4.min.js"></script>
    <script src="~/Theme/assets/js/bootstrap.min.js"></script>
    <script src="~/Theme/assets/js/chosen.min.js"></script>
    <script src="~/Theme/assets/js/countdown.min.js"></script>
    <script src="~/Theme/assets/js/jquery.scrollbar.min.js"></script>
    <script src="~/Theme/assets/js/lightbox.min.js"></script>
    <script src="~/Theme/assets/js/magnific-popup.min.js"></script> 
    <script src="~/Theme/assets/js/slick.js"></script>
    <script src="~/Theme/assets/js/jquery.zoom.min.js"></script>
    <script src="~/Theme/assets/js/threesixty.min.js"></script>
    <script src="~/Theme/assets/js/jquery-ui.min.js"></script>
    <script src="~/Theme/assets/js/mobilemenu.js"></script>    
    <script src="~/Theme/assets/js/functions.js"></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    <script src="~/Theme/assets/js/plugins.js"></script>

    <script src="~/Theme/assets/js/custom.js"></script>    

    <script>
        $(document).ready(function () {
            updateCart();
        });
    </script>
</body>

</html>